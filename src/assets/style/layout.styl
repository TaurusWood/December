body, html, div, ul
  font-family Helvetica, arial, sans-serif;
  font-size: 14px
  margin 0
  padding 0
  box-sizing border-box

body
  background-color: #660ac1;
  overflow-x: hidden

ul
  li
    list-style none

p
  margin: 0 0 10px

img
  width 100%
  height 100%

a
  display: block;
  text-decoration none
  color initial
  &:focus
    outline none
